<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zlzhang.stockanalysis">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".list.view.StockListActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".upload.view.UploadActvity"
            android:screenOrientation="portrait" />

        <activity android:name=".analysis.average.view.AverageActivity"
            android:screenOrientation="portrait" />

        <activity android:name=".analysis.rise.view.ContinueRiseActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>